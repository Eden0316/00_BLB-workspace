	    <result column="RCVR_ADDRESS" property="rcvrAddress" />
	    <result column="DLVR_REQ_MESSAGE" property="dlvrReqMessage" />
	    <result column="DLVR_STATUS" property="dlvrStatus" />
	    <result column="DLVR_FEE" property="dlvrFee" />
